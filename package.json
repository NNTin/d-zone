{
  "name": "d-zone",
  "version": "0.0.7",
  "description": "An ambient life simulation driven by the user activity within a Discord server",
  "main": "index.js",
  "dependencies": {
    "express": "^5.1.0",
    "inherits": "^2.0.4",
    "raf": "^3.1.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "brfs": "^2.0.1",
    "browserify": "^17.0.0",
    "concurrently": "^9.2.1",
    "terser": "^5.44.0",
    "watchify": "^4.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dev": "browserify src/main.js -t brfs -o dist/static/bundle.js && terser dist/static/bundle.js -o dist/static/bundle.min.js",
    "build:prod": "browserify src/main.js -t brfs | terser -c -m -o dist/static/bundle.min.js",
    "watch": "watchify src/main.js -t brfs -d -o dist/static/bundle.min.js -v",
    "prestart": "npm run build:dev",
    "start": "node index.js",
    "dev": "concurrently \"npm run watch\" \"npm start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NNTin/d-zone.git"
  },
  "keywords": [
    "discord",
    "Discord",
    "isometric",
    "canvas"
  ],
  "author": "Tin Nguyen",
  "contributors": [
    "Devin Spikowski (https://github.com/vegeta897)"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NNTin/d-zone/issues"
  },
  "homepage": "https://nntin.github.io/d-zone"
}
